                    <p><%= pet.type %> pet</p>
                    <pre>Pet:                                <%= pet.pet %></pre>
                    <% if (pet.petname) { %>
                        <pre>Pet's Name:                 <%= pet.petname %></pre>
                    <% } %>
                    <% if (pet.age) { %>
                        <pre>Age:                               <%= pet.age %></pre>
                    <% } %>
                    <% if (pet.gender) { %>
                        <pre>Gender:                        <%= pet.gender %></pre>
                    <% } %>
                    <% if (pet.colour) { %>
                        <pre>Color:                            <%= pet.colour %></pre>
                    <% } %>
                    <pre>Owner's Name:       </pre>
                    <pre>Owner's Number:     </pre>
                    <% if (pet.date) { %>
                        <pre>Date:                             <%= pet.date.toString().split(' ').slice(0, 4).join(' ') %></pre>
                    <% } %>
                    <% if (pet.address) { %>
                        <pre>Address:                       <%= 
                            (function() {
                                var addressParts = pet.address.split(',');
                                if (addressParts.length >= 3) {
                                    var firstPart = addressParts[1].trim();
                                    var secondPart = addressParts[2].trim();
                                    return firstPart + ', ' + secondPart;
                                } else {
                                    return pet.address;
                                }
                            })()
                        %></pre>
                    <% } %>
                    <% if (pet.breed) { %>
                        <pre>Breed:                           <%= pet.breed %></pre>
                    <% } %>
                    <% if (pet.size) { %>
                        <pre>Size:                               <%= pet.size %></pre>
                    <% } %>
                </div>
                
            </div>
