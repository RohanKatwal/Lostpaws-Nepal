<%- include("../components/head") %>
<body>
    <%- include("../components/nav") %>
    <div class="container">
        <div class="claim-heading">
            <p><%= pet.petname ? "My Name is " + pet.petname : 'Help me' %></p>
                <% if (pet.type === 'Lost' || pet.type === 'Found') { %>
                    <button class="claim-btn">
                        <% if (pet.type === 'Lost') { %>
                            I have found it
                        <% } else if (pet.type === 'Found') { %>
                            I have lost it
                        <% } %>
                    </button>
                <% } %>
        </div>
        <div class="info">
            <div class="pet-n-reward">
                <div class="pet-info-img">
                    <img id="largeImage" src="/<%=pet.images[0].path %>" alt="Large Image">
                </div>
            
                <!-- Thumbnail images -->
                <div class="thumbnail-section">
                    <% pet.images.forEach(function(image) { %>
