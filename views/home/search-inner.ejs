<script>
    
function showImage(imageSrc) {
    var largeImage = document.getElementById('largeImage');
    // Check if the imageSrc starts with "/uploads/", if not, add it
    if (imageSrc.indexOf("/uploads") === 0) {
        var firstSlashIndex = imageSrc.indexOf("/", 1); // Find the index of the first slash after "/uploads"
        if (firstSlashIndex === -1) {
            // If no slash is found after "/uploads", insert one
            imageSrc = imageSrc.slice(0, 8) + "/" + imageSrc.slice(8);
        }
    }
    largeImage.src = imageSrc;
}

// function showImage(imageSrc) {
//     var largeImage = document.getElementById('largeImage');
//     largeImage.src = imageSrc;
// }
var profileImageSrc = $('#userProfile').val();


document.querySelectorAll('.view-reply-btn').forEach(btn => {
    btn.addEventListener("click", function() {
        const repliesSection = btn.nextElementSibling;
        if (repliesSection.style.display === "none" || repliesSection.style.display === "") {
            repliesSection.style.display = "block";
        } else {
            repliesSection.style.display = "none";
        }
    });
});


document.addEventListener("DOMContentLoaded", function() {
    const likeButtons = document.querySelectorAll(".like-btn");
    const replyButtons = document.querySelectorAll(".reply-btn");

    likeButtons.forEach(btn => {
    btn.addEventListener("click", function() {
        const likesCount = parseInt(btn.dataset.likes) || 0;
        btn.innerHTML = `
        <svg width="18" height="18" viewBox="0 0 18 18" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M0.75 16.5H3.75V7.5H0.75V16.5ZM17.25 8.25C17.25 7.425 16.575 6.75 15.75 6.75H11.0175L11.73 3.3225L11.7525 3.0825C11.7525 2.775 11.625 2.49 11.4225 2.2875L10.6275 1.5L5.6925 6.4425C5.415 6.7125 5.25 7.0875 5.25 7.5V15C5.25 15.825 5.925 16.5 6.75 16.5H13.5C14.1225 16.5 14.655 16.125 14.88 15.585L17.145 10.2975C17.2125 10.125 17.25 9.945 17.25 9.75V8.25Z" fill="black"/>
                        </svg> ${likesCount + 1}`;
        btn.dataset.likes = likesCount + 1;
    });
});

replyButtons.forEach(btn => {
    btn.addEventListener("click", function() {
        const parentComment = btn.closest(".total-comments");
        const replySection = parentComment.querySelector(".replies-section");

        // Check if reply section already contains textarea and button
        if (!replySection.querySelector("input") && !replySection.querySelector("button")) {
            const replyInput = document.createElement("input");
            replyInput.placeholder = "Write your reply here...";
            const submitButton = document.createElement("button");
            submitButton.textContent = "reply";
            submitButton.addEventListener("click", function() {
                const replyText = replyInput.value;
                if (replyText.trim() !== "") {
                    const reply = document.createElement("div");
                    reply.classList.add("reply");
                    reply.innerHTML = `
                        <div class="reply-user-profile">
                            <img src="${profileImageSrc}" alt="">
                        </div>
                        <div>
                            <p class="username">Your Username</p>
                            <p class="reply-comment">${replyText}</p>
                        </div>
                    `;
                    replySection.prepend(reply);
                    replyInput.value = "";
                }
            });
            replySection.appendChild(replyInput);
            replySection.appendChild(submitButton);
        } else {
            // If textarea and button already exist, focus on the textarea
            replySection.querySelector("input").focus();
        }
    });
});

});


</script>
